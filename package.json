{
  "name": "is-in-subnet",
  "version": "1.5.0",
  "description": "Check if an IPv4 or IPv6 address is contained in the given CIDR subnet",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "test": "npm run prepare && ava --verbose ./build/test",
    "coverage": "npm run prepare && nyc --reporter lcov --reporter text ava ./build/test",
    "prepare": "del-cli build && tsc"
  },
  "engines": {
    "node": ">=6.10.3"
  },
  "repository": "natesilva/is-in-subnet",
  "keywords": [
    "ip",
    "address",
    "ipv4",
    "ipv6",
    "cidr",
    "networking"
  ],
  "license": "MIT",
  "author": "Nate Silva <nate@natesilva.com>",
  "devDependencies": {
    "@types/node": "^10.5.7",
    "ava": "^0.25.0",
    "del-cli": "^1.1.0",
    "nyc": "^12.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
