{
  "name": "is-in-subnet",
  "version": "1.7.0",
  "description": "Check if an IPv4 or IPv6 address is contained in the given CIDR subnet",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "test": "ava --verbose",
    "coverage": "nyc --reporter lcov --reporter text --extension .ts ava",
    "prepare": "del-cli build && tsc",
    "prettier": "prettier --write \"{src,test}/**/*.?(ts|json)\""
  },
  "engines": {
    "node": ">=6.10.3"
  },
  "repository": "natesilva/is-in-subnet",
  "keywords": [
    "ip",
    "address",
    "ipv4",
    "ipv6",
    "cidr",
    "networking"
  ],
  "license": "MIT",
  "author": "Nate Silva <nate@natesilva.com>",
  "devDependencies": {
    "@types/node": "^10.12.2",
    "ava": "^1.0.0-rc.1",
    "del-cli": "^1.1.0",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  }
}
